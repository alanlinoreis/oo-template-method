<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>11</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <!-- Incluir recursivamente arquivos .cs das pastas irmãs dentro de src/ -->
    <!-- Excluir bin/ e obj/ gerados para evitar arquivos especificados múltiplas vezes -->
    <Compile Include="..\**\*.cs" Exclude="..\Core\**\*;..\**\bin\**;..\**\obj\**" />
  </ItemGroup>
</Project>